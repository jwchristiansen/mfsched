div
  = form_with url: programs_path, multipart: true, data: { controller: "disable", "disable-with-value": "Processing..." } do |form|
    div.mb-5 = form.file_field :csv
    div = form.submit "Generate Schedule >", class: 'bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded cursor-pointer'
